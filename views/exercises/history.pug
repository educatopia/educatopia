extends layout

block thirdTab
  table.table.table-striped.table-condensed
    thead
      tr
        th Revision
        th Created
        th User
    tbody
      each exercise, index in history
        tr
          td= index + 1
          td= exercise.createdAt.toISOString()
          td
            a(href='/' + exercise.createdBy)
              =exercise.createdBy
